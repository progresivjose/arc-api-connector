{
    "name": "progresivjose/arc-api-connector",
    "description": "A simple Guzzle Wrapper with some specific params to connect to Arc XP API",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Progresivjose\\ArcApiConnector\\": "src/"
        }
    },
    "require-dev": {
        "pestphp/pest": "1.x",
        "mockery/mockery": "~1.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
