{
    "name": "progresivjose/arc-api-connector",
    "description": "A simple Guzzle Wrapper with some specific params to connect to Arc XP API",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=8.2.11",
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Progresivjose\\ArcApiConnector\\": "src/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.24",
        "mockery/mockery": "^1.4.4"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
